{
    "name": "KFCLibrary",
    "version": "0.1.3",
    "authors": {
        "name": "sascha lammers",
        "maintainer": true
    },
    "frameworks": "arduino",
    "platforms": [
        "espressif8266",
        "espressif32"
    ],
    "build": {
        "srcFilter": [
            "-<KFCBaseLibrary/src/mock/>",
            "-<KFCBaseLibrary/src/VirtualPin/>"
        ],
        "build": {
            "includeDir": [
                "KFCBaseLibrary/include",
                "KFCConfiguration/src",
                "KFCEventScheduler/include",
                "KFCGfx/include",
                "KFCJson/include",
                "KFCOpenWeather/src",
                "KFCPinMonitor/src",
                "KFCResetDetector/include",
                "KFCRestApi",
                "KFCSyslog/include",
                "KFCSyslog/include",
                "KFCWebFramework/include",
                "stl_ext/include"
            ],
            "srcDir": [
                "KFCBaseLibrary/src",
                "KFCConfiguration/src",
                "KFCEventScheduler/src",
                "KFCGfx/src",
                "KFCJson/src",
                "KFCOpenWeather/src",
                "KFCPinMonitor/src",
                "KFCResetDetector/src",
                "KFCRestApi",
                "KFCSyslog/src",
                "KFCWebBuilder/src",
                "KFCWebFramework/src",
                "stl_ext/src"
            ]
        }
    }
}
