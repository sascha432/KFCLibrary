{
  "name": "KFCLibrary",
  "version": "0.1.4",
  "authors": {
    "name": "sascha lammers",
    "maintainer": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sascha432/KFCLibrary.git"
  },
  "frameworks": [
    "espidf",
    "arduino"
  ],
  "platforms": [
    "atmelavr",
    "espressif8266",
    "espressif32"
  ],
  "runnable": true,
  "dependencies": [
    {
      "name": "libcrc16",
      "version": "https://github.com/sascha432/libcrc16.git"
    }
  ],
  "build": {
    "srcDir": ".",
    "extraScript": "./scripts/extra_script.py",
    "flags": [
      "-I./KFCBaseLibrary/include",
      "-I./KFCConfiguration/include",
      "-I./KFCEventScheduler/include",
      "-I./KFCGfx/include",
      "-I./KFCJson/include",
      "-I./KFCOpenWeather/include",
      "-I./KFCPinMonitor/src",
      "-I./KFCResetDetector/include",
      "-I./KFCRestApi",
      "-I./KFCSyslog/include",
      "-I./KFCWebBuilder/include",
      "-I./KFCWebFramework/include",
      "-I./stl_ext/include",
      "-D _POSIX_C_SOURCE=200809L",
      "-D __GNU_VISIBLE=1",
      "-D __MISC_VISIBLE=1",
      "-D KFC_LIBRARY_VERSION=1003",
      "-D DEBUG_OUTPUT=Serial"
    ],
    "srcFilter": [
      "+<./KFCBaseLibrary/src>",
      "+<./KFCConfiguration/src>",
      "+<./KFCEventScheduler/src>",
      "+<./KFCJson/src>",
      "+<./KFCOpenWeather/src>",
      "+<./KFCPinMonitor/src>",
      "+<./KFCResetDetector/src>",
      "+<./KFCRestApi>",
      "+<./KFCSyslog/src>",
      "+<./KFCWebBuilder/src>",
      "+<./KFCWebFramework/src>",
      "+<./KFCGfx/src>",
      "+<./stl_ext/src>"
    ],
    "libArchive": false
  }
}
